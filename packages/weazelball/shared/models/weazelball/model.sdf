<?xml version="1.0" ?>
<sdf version="1.4">
  <model name="weazelball">
    <link name="shell">
      <self_collide>false</self_collide>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <inertia>
          <ixx>0.00000682197</ixx>
          <ixy>-0.0000000006</ixy>
          <ixz>0.0000000003</ixz>
          <iyy>0.00000728564</iyy>
          <iyz>0.0000000005</iyz>
          <izz>0.00000682695</izz>
        </inertia>
        <mass>0.0067</mass>
      </inertial>
      <visual name="shell_1_geom_visual">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <mesh>
            <uri>model://weazelball/meshes/visual/shell_ctl_side.dae</uri>
          </mesh>
        </geometry>
      </visual>
      <visual name="shell_2_geom_visual">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <mesh>
            <uri>model://weazelball/meshes/visual/shell_joint_side.dae</uri>
          </mesh>
        </geometry>
      </visual>
      <collision name="base_geom_collision">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <sphere>
            <radius>0.041</radius>
          </sphere>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>2.0</mu>
              <mu2>2.0</mu2>
              <fdir1>0 0 1</fdir1>
              <slip1>0.0</slip1>
              <slip2>0.0</slip2>
            </ode>
            <bullet>
              <friction>2.0</friction>
              <friction2>2.0</friction2>
              <fdir1>0 0 0</fdir1>
              <rolling_friction>2.0</rolling_friction>
            </bullet>
          </friction>
        </surface>
      </collision>
    </link>
    <link name="motor">
      <self_collide>false</self_collide>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <inertia>
          <ixx>0.00002149676</ixx>
          <ixy>0.00000224448</ixy>
          <ixz>-0.00000127349</ixz>
          <iyy>0.00002112172</iyy>
          <iyz>0.00000041827</iyz>
          <izz>0.00002531389</izz>
        </inertia>
        <mass>0.085</mass>
      </inertial>
      <visual name="motor_geom_visual">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <mesh>
            <uri>model://weazelball/meshes/visual/motor.dae</uri>
          </mesh>
        </geometry>
      </visual>
    </link>

    <joint name="motor_actuator" type="revolute"> 
      <parent>shell</parent>
      <child>motor</child>
      <axis>
        <dynamics>
          <damping>0.0</damping>
          <friction>0.0</friction>
        </dynamics>
        <xyz>0 1 0</xyz>
<!--
        <limit>
          <lower>0</lower>
          <upper>0</upper>
        </limit>
-->
<!--
        <limit>
          <lower>0</lower>
          <upper>0.014</upper>
        </limit>
-->
      </axis>
    </joint>

    <!-- Fixed base -->
