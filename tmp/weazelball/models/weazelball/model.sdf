<?xml version="1.0" ?>
<sdf version="1.4">
  <model name="weazelball">
    <link name="shell">
      <!-- shell -->
      <pose>0 -0.5 0 0 0 0</pose>
      <gravity>0</gravity>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <inertia>
          <ixx>0.0016667</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.0016667</iyy>
          <iyz>0</iyz>
          <izz>0.0016667</izz>
        </inertia>
        <mass>1.0</mass>  <!-- (g) -->
      </inertial>
      <collision name="shell_col">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size>0.1 0.1 0.1</size>
          </box>
        </geometry>
      </collision>
      <visual name="shell_vis">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size>0.1 0.1 0.1</size>
          </box>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/Blue</name>
          </script>
        </material>
      </visual>
    </link>
    <link name="axel">
      <!-- cylinder -->
      <pose>0 -0.5 0 0 0 0</pose>
      <gravity>0</gravity>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <inertia>
          <ixx>0.0016667</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.0016667</iyy>
          <iyz>0</iyz>
          <izz>0.0016667</izz>
        </inertia>
        <mass>1.0</mass>  <!-- (g) --> 
      </inertial>
      <collision name="axel_col">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size>0.1 0.1 0.1</size>
          </box>
        </geometry>
      </collision>
      <visual name="axel_vis">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size>0.1 0.1 0.1</size>
          </box>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/Blue</name>
          </script>
        </material>
      </visual>
    </link>
    <link name="motor">
      <pose>-16.81 1.36 -1.70 0 0 0</pose>  <!-- com of motor (mm) -->
      <gravity>0</gravity>
      <!-- com and aligned with output coordinate system (g * mm^2) -->
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <inertia>
          <ixx>21496.76</ixx>
          <ixy>2244.48</ixy>
          <ixz>-1273.49</ixz>
          <iyy>21121.72</iyy>
          <iyz>418.27</iyz>
          <izz>25313.89</izz>
        </inertia>
      <!-- at output coordinate system (g * mm^2) -->
<!--
        <inertia>
          <ixx>21599.96</ixx>
          <ixy>300.72</ixy>
          <ixz>1156.60</ixz>
          <iyy>45384.67</iyy>
          <iyz>221.60</iyz>
          <izz>49488.27</izz>
        </inertia>
-->
        <mass>85</mass>  <!-- (g) -->
      </inertial>
      <collision name="motor_col">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size>0.1 0.1 0.1</size>
          </box>
        </geometry>
      </collision>
      <visual name="motor_vis">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size>0.1 0.1 0.1</size>
          </box>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/Blue</name>
          </script>
        </material>
      </visual>
    </link>
    <link name="gasket">
      <!-- torus -->
      <pose>0 0 0 0 0 0</pose>
      <gravity>0</gravity>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <inertia>
          <ixx>0</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0</iyy>
          <iyz>0</iyz>
          <izz>0</izz>
        </inertia>
        <mass>0</mass>  <!-- (g) -->
      </inertial>
      <collision name="gasket_col">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size>0.1 0.1 0.1</size>
          </box>
        </geometry>
      </collision>
      <visual name="gasket_vis">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size>0.1 0.1 0.1</size>
          </box>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/Black</name>
          </script>
        </material>
      </visual>
    </link>

    <!-- fixed joint joining axel to shell -->
    <!-- fixed joint joining gasket to shell -->
    <joint name="actuator" type="revolute"> 
      <parent>shell</parent>
      <child>motor</child>
      <axis>
        <dynamics>
          <damping>0.0</damping>
          <friction>0.0</friction>
        </dynamics>
        <xyz>0 1 0</xyz>
      </axis>
    </joint>

    <plugin name='controller' filename='libcontroller.so'>
    </plugin>

  </model>
</sdf>
